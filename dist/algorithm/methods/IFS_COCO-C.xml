<algorithm_specification>
  <name>IFS-COCO</name>
  <nParameters>7</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>200</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>K</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>30</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Max Evaluations</name>
    <type>integer</type>
    <domain>
      <lowerB>100</lowerB>
      <upperB>1000000</upperB>
    </domain>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>MODE</name>
    <type>list</type>
    <domain>
      <item>ALL</item>
      <item>SI</item>
      <item>SC</item>
      <item>SCI</item>
	</domain>
    <default>ALL</default>   
  </parameter>  
  <parameter>
    <name>Alpha</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>Beta</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.99</default>
  </parameter>
  <parameter>
    <name>Prob 0 to 1</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.25</default>
  </parameter>
</algorithm_specification>
