<algorithm_specification>
  <name>MOPNAR</name>
  <nParameters>9</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
   <parameter>
    <name>Number of Objetives</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>3</upperB>
    </domain>
    <default>3</default>
  </parameter>
   <parameter>
    <name>Number of Evaluations</name>
    <type>integer</type>
    <domain>
      <lowerB>100</lowerB>
      <upperB>1000000</upperB>
    </domain>
    <default>50000</default>
  </parameter>
  <parameter>
    <name>H control parameter</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>13</default>
  </parameter>
  <parameter>
    <name>Weight vectors in neighborhood</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Probability parent solutions are selected from the neighborhood</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.9</default>
  </parameter>
  <parameter>
    <name>Max solutions replaced by each child solution</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>2</default>
  </parameter>
  <parameter>
    <name>Probability of Mutation</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.1</default>
  </parameter>   
  <parameter>
    <name>Factor amplitude</name>
    <type>real</type>
    <domain>
      <lowerB>2.0</lowerB>
      <upperB>20.0</upperB>
    </domain>
    <default>2.0</default>
  </parameter>
  <parameter>
    <name>Difference threshold</name>
    <type>real</type>
    <domain>
      <lowerB>5.0</lowerB>
      <upperB>100.0</upperB>
    </domain>
    <default>5.0</default>
  </parameter>
 </algorithm_specification>
