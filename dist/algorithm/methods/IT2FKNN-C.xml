<algorithm_specification>
  <name>Interval Type-2 Fuzzy K Nearest Neighbors Classifier</name>
  <nParameters>3</nParameters>
  <seed>0</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>K Value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>M</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>100.0</upperB>
    </domain>
    <default>2.0</default>
  </parameter>
  <parameter>
    <name>k Max</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>9</default>
  </parameter>
</algorithm_specification>
