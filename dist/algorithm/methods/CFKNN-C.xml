<algorithm_specification>
  <name>Condensed Fuzzy K Nearest Neighbors Classifier</name>
  <nParameters>2</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>K Value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Alpha</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>10.0</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
</algorithm_specification>
