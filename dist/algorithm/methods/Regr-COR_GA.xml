<algorithm_specification>
  <name>Fuzzy Rule Learning, COR Algorithm</name>
  <nParameters>11</nParameters>
  <seed>1</seed>
  <nOutput>2</nOutput>
  <parameter>
    <name>Agrupation of examples</name>
    <type>list</type>
    <domain>
      <item>WM</item>
      <item>CH</item>
    </domain>
    <default>WM</default>
  </parameter>
  <parameter>
    <name>Obtention of candidate consequents</name>
    <type>list</type>
    <domain>
      <item>WM</item>
      <item>CH</item>
    </domain>
    <default>WM</default>
  </parameter>  
  <parameter>
    <name>Delete Rules</name>
    <type>list</type>
    <domain>
      <item>YES</item>
      <item>NO</item>
    </domain>
    <default>YES</default>
  </parameter>
  <parameter>
    <name>Learn Weights</name>
    <type>list</type>
    <domain>
      <item>YES</item>
      <item>NO</item>
    </domain>
    <default>YES</default>
  </parameter>
  <parameter>
    <name>Number of labels</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Weight Cross Type</name>
    <type>list</type>
    <domain>
		<item>MMA</item>
		<item>2pConj</item>
		<item>2pDisj</item>
    </domain>
    <default>MMA</default>
  </parameter>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>51</default>
  </parameter>
  <parameter>
    <name>Number of Generations</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Crossover Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>  
  <parameter>
    <name>Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.4</default>
  </parameter>  
  <parameter>
    <name>Parameter-A (MMA Crossover)</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>    
</algorithm_specification>
