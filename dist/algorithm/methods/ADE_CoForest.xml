<algorithm_specification>
  <name>ADE_CoForest</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>

    <parameter>
    <name>Number of Classifiers</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10</upperB>
    </domain>
    <default>6</default>
  </parameter>
  
   <parameter>
    <name>threshold</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.75</default>
  </parameter>

  <parameter>
    <name>Number of Neighbors</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>3</default>
  </parameter>

   <parameter>
    <name>Minimun number of neighbours with the same class</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>2</default>
  </parameter>

</algorithm_specification>
