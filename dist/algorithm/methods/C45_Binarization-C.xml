<algorithm_specification>
  <name>C4.5-OVO</name>
  <nParameters>6</nParameters>
  <seed>0</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>pruned</name>
    <type>list</type>
    <domain>
      <item>TRUE</item>
      <item>FALSE</item>
    </domain>
    <default>TRUE</default>
  </parameter>
  <parameter>
    <name>confidence</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.25</default>
  </parameter>
  <parameter>
    <name>instancesPerLeaf</name>
    <type>integer</type>
    <default>2</default>
  </parameter>
  <parameter>
    <name>Binarization</name>
    <type>list</type>
    <domain>
      <item>OVO</item>
      <item>OVA</item>
    </domain>
    <default>OVO</default>
  </parameter>  
  <parameter>
    <name>Score Function</name>
    <type>list</type>
    <domain>
      <item>VOTE</item>
      <item>WEIGHTED</item>
      <item>DDAG</item>
      <item>PC</item>
      <item>ND</item>
      <item>LVPC</item>
      <item>BTS</item>
      <item>WuJMLR</item>
      <item>NESTING</item>      
      <item>DynOVO</item>
    </domain>
    <default>WEIGHTED</default>
  </parameter>
  <parameter>
    <name>BTS_Param (if applied)</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.05</default>
  </parameter>  
</algorithm_specification>
