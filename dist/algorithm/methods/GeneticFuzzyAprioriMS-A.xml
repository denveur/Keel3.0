<algorithm_specification>
  <name>GeneticFuzzyAprioriMS</name>
  <nParameters>12</nParameters>
  <seed>1</seed>
  <nOutput>2</nOutput>
  <parameter>
    <name>Number of Evaluations</name>
    <type>integer</type>
    <domain>
      <lowerB>100</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>Probability of Mutation</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
  <parameter>
    <name>Probability of Crossover</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.8</default>
  </parameter>
  <parameter>
    <name>Parameter d for MMA Crossover</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.35</default>
  </parameter>    
  <parameter>
    <name>Maximum Minimum Support</name>
    <type>real</type>
    <domain>
      <lowerB>-1.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>-1.0</default>
  </parameter>
  <parameter>
    <name>Use Max Operator for 1-Frequent Itemsets</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>Minimum Confidence</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.8</default>
  </parameter>
  <parameter>
    <name>Number of clusters</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>20</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Interval Threshold</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.3</default>
  </parameter>
  <parameter>
    <name>Break Threshold</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>100.0</upperB>
    </domain>
    <default>5.0</default>
  </parameter>
  <parameter>
    <name>Percentage of the required number of large 1-itemsets</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.8</default>
  </parameter>
</algorithm_specification>
