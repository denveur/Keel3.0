<algorithm_specification>
  <name>SelfTraining</name>
  <nParameters>6</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>



  <parameter>
    <name>Number of selected examples</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>1</default>
  </parameter>

   <parameter>
    <name>MaxIter</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>40</default>
  </parameter>
  
   <parameter>
    <name>Initial Pool</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>75</default>
  </parameter>
  

   <parameter>
    <name>Classifier1</name>
    <type>list</type>
    <domain>
      <item>NN</item>
      <item>C45</item>
      <item>NB</item>	
      <item>SMO</item>
    </domain>
    <default>NN</default>   
  </parameter>
  
   <parameter>
    <name>Classifier2</name>
    <type>list</type>
    <domain>
      <item>NN</item>
      <item>C45</item>
      <item>NB</item>	
      <item>SMO</item>
    </domain>
    <default>C45</default>   
  </parameter>

   <parameter>
    <name>Final Classifier</name>
    <type>list</type>
    <domain>
      <item>NN</item>
      <item>C45</item>
      <item>NB</item>	
      <item>SMO</item>
    </domain>
    <default>NN</default>   
  </parameter>
</algorithm_specification>
